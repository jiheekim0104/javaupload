package javaBasic1;

public class ForEx {

	public static void main(String[] args) {
		int n = 5;
		while (n <= 10) {
			System.out.println(n);
			n++;
		}

		System.out.println("------------------------------");

		int a = 10;
		while (a >= 5) {
			System.out.println(a + " ");
			a++;
			if (a == 13)
				break;
		}

		System.out.println("------------------------------");

		int num = 1;
		while (num <= 10) {
			System.out.print(num + " ");
			num++;
		}

		System.out.println("------------------------------");
		
		int num1 = 0;
		while (num1 <= 100) {
			System.out.print(num1 + " ");
			num1=num1+5;
		}
		
		System.out.println("------------------------------");
		
		int num2 = -100;
		while (num2 <=100) {
			System.out.print(num2 + " ");
			num2 = num2 +50;
			
		}
		
		char text = 'A';
		while ( text <= 'Z') {
			System.out.println(text+" ");
			text++;
		} 
		
		/* int textnumber = 65;
		while (textnumber <= 90) {
			char text = (char)textnumber;
			System.out.println(text);
			textnumber = textnumber=1; */
		}
		
				
		
		

	}
}
