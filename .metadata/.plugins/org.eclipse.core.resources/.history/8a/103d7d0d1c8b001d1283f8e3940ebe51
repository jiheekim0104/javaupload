package javaBasic1;

public class VarEx {

	public static void main(String[] args) {
		System.out.println('일');
		System.out.println("여러글자");
		System.out.println("옹");
		
		int a;
		a = 10;
		
		String hakbun;
		String name;
		String dept;
		int age;
		boolean sex;
		double jumsu;
		Doctor doctor; 프로그래머가 만들어준 객체 자료형
		Programmer programmer;
		
	}

}
	class Doctor{ 요기 밑에서 정의해주지롱
		
	}
	
	class Programmer {
		
	}