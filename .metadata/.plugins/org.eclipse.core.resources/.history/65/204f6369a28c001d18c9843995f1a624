package ch02.sec01;

import java.util.Scanner;

public class VarEx30 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in); 

		System.out.print("몸무게:");
		String mom  =sc.next();
		
		System.out.print("키:");
		String key  =sc.next();
		
		System.out.print("성별:");
		String eng  =sc.next();
		
		
		int mom1 = Integer.parseInt(mom);
		int key1 = Integer.parseInt(key);
		int cul = mom1/key1*key1;
		
		System.out.println("-------------------------");
		System.out.println("당신의 체질향 지수는:"+" "+ cul +"입니다.");
		System.out.println("-------------------------");
	}

}
