package javaBasic1;

public class ForEx {

	public static void main(String[] args) {
		int n =5;
		while (n<=10) {
			System.out.println(n);
			n++;
			
			int a =10;
			while (a>=5) {
				System.out.println(a+" ");
				a++;
				break;
			}
			
			int num =1;
			while (num<=10) {
				System.out.println(num+" ");
				num++;
			}
			} 
		}
	}


