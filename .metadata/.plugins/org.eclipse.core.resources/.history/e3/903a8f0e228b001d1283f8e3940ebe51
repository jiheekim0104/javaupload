package exBasic0;

public class MySecond {

	public static void main(String[] args) {
		System.out.println("자바는");
		System.out.println("시스템에 독립적인");
		System.out.println("객체지향 프로그래밍 언어이다");
		
		System.out.println("-----------");
		
		System.out.println("자바는 \n 시스템에 독립적인 \n 객체지향 프로그래밍 언어이다");
		
		System.out.println("-----------");
		
		String a;
		String b;
		String c;
		
		a = "자바는";
		b = "시스템에 독립적인";
		c = "객체지향 프로그래밍 언어이다";
		
		System.out.println(a);
		System.out.println(b);
		System.out.println(c);
	}

}
