package ch02.sec01;

import java.util.Scanner;

public class VarEx3 {

	public static void main(String[] args) {
		/*이름 : 홍길동
		국어 : 100
		영어 : 80
		수학 : 90
		
		홍길동 100 80 90 270 90 1등 */
		
		Scanner sc = new Scanner(System.in); 

		System.out.print("이름:");
		String name  =sc.next();
		
		
		System.out.println("-------------------------");
		System.out.println(name);
		System.out.println("-------------------------");
	}

}
