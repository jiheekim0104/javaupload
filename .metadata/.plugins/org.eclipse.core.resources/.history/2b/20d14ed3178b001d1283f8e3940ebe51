package javaBasic1;

public class VarEx {

	public static void main(String[] args) {
		// 첫글자가 대문자면 객체이다 ㅇㅅㅇ .  . ~안에) 
		// () 소 : 함수(~하라, 실행하라, 동작하라)  
		// {} 중 : 함수의 처리 내용을 입력하는 곳 
		// [] 대 : 배열 (행렬의 기호를 떠올리면됨 ㅇㅇ) 
		// <> 다 : 내용을 묶어서 라벨링 해주는 역할 
		System.out.println('일');
		System.out.println("여러글자");
	}

}
